void main() {
  // Membuat simbol
  Symbol nameSymbol = Symbol('Sukma Suk');
  Symbol ageSymbol = Symbol('Umur');

  // Menggunakan simbol dalam peta
  Map<Symbol, String> SukmaInfo = {
    nameSymbol: 'Muhammmad Sukma Atmaja',
    ageSymbol: '21 tahun',
  };

  // Menampilkan informasi menggunakan simbol
  print('${nameSymbol}: ${SukmaInfo[nameSymbol]}');
  print('${ageSymbol}: ${SukmaInfo[ageSymbol]}');
}
