void main() {
  // Daftar nama yang hanya berisi nama SUKMA A
  List<String> names = ['SUKMA A'];

  // Menggunakan fungsi anonim untuk memfilter nama yang mengandung "SUKMA"
  var filteredNames = names.where((name) => name.contains('SUKMA')).toList();

  print('Nama yang mengandung "SUKMA":');
  filteredNames.forEach((name) {
    print(name); // Menampilkan nama yang mengandung "SUKMA"
  });

  // Menggunakan fungsi anonim untuk mengubah nama menjadi huruf kecil
  var lowerCaseNames = names.map((name) => name.toLowerCase()).toList();

  print('Nama dalam huruf kecil:');
  lowerCaseNames.forEach((name) {
    print(name); // Menampilkan nama dalam huruf kecil
  });
}
