void main() {
  // Membuat list dengan nama "Sukma" untuk menyimpan daftar buku
  List<String> sukmaBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku Sukma: ' + sukmaBooks.toString());

  // Menambahkan buku baru
  sukmaBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + sukmaBooks.toString());

  // Menghapus buku
  sukmaBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + sukmaBooks.toString());

  // Mengakses elemen list
  print('Buku pertama sukma: ' + sukmaBooks[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku sukma: ' + sukmaBooks.length.toString());
}
